<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainBgColor"
    tools:context=".PersonalCaseActivity2">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="322dp"
        android:layout_height="380dp"
        android:src="@drawable/circle_down_left_big"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="365dp"
        android:layout_height="365dp"
        android:src="@drawable/circle_up_right_big"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/randevu_word"
        app:layout_constraintBottom_toTopOf="@+id/personalCase2ImageButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/personalCase2ImageButton"
        android:layout_width="156dp"
        android:layout_height="196dp"
        android:layout_marginBottom="12dp"
        android:onClick="onClickAddPhoto"
        app:layout_constraintBottom_toTopOf="@+id/textViewHelper"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/profile_sec"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/personalCase2ContinueButton"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/rounded_mplus_bold"
        android:onClick="onClickContinue"
        android:text="Продолжить"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.907"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textViewHelper"
        android:layout_width="361dp"
        android:layout_height="52dp"
        android:layout_marginBottom="20dp"
        android:text="Нажмите на иконку что бы добававить фотографию"
        android:textColor="#FFFFFFFF"
        android:textSize="22dp"
        app:layout_constraintBottom_toTopOf="@+id/personalCase2DescriptionEditText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/personalCase2DescriptionEditText"
        android:layout_width="374dp"
        android:layout_height="134dp"
        android:layout_marginBottom="32dp"
        android:background="@color/white"
        android:ems="11"
        android:fontFamily="@font/diffont"
        android:hint="Напишите о себе"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        android:textAlignment="gravity"
        android:textColorHint="#FF000000"
        app:layout_constraintBottom_toTopOf="@+id/personalCase2ContinueButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>