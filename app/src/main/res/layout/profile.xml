<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ProfileActivity">

    <ImageView
        android:id="@+id/profileLayoutPhotoImageView"
        android:layout_width="279dp"
        android:layout_height="315dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.298"
        app:srcCompat="@drawable/profile_sec"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/profileLayoutMainInfoTextView"
        android:layout_width="290dp"
        android:layout_height="29dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:text="Имя"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileLayoutPhotoImageView" />

    <ImageButton
        android:id="@+id/imageView3"
        android:layout_width="42dp"
        android:layout_height="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:onClick="onClickSettings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group" />

    <TextView
        android:id="@+id/profileLayoutDescriptionTextView"
        android:layout_width="272dp"
        android:layout_height="71dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:text="Description"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileLayoutSecondInfoTextView" />

    <TextView
        android:id="@+id/profileLayoutSecondInfoTextView"
        android:layout_width="288dp"
        android:layout_height="33dp"
        android:layout_marginTop="12dp"
        android:text="SecondInfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileLayoutMainInfoTextView" />

    <ImageButton
        android:id="@+id/imageView6"
        android:layout_width="51dp"
        android:layout_height="53dp"
        android:layout_marginStart="160dp"
        android:layout_marginBottom="40dp"
        android:onClick="onClickLike"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.235"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:srcCompat="@drawable/heart" />

    <ImageButton
        android:id="@+id/imageView8"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="40dp"
        android:onClick="onClickMessage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/group" />

    <Button
        android:id="@+id/buttonOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:onClick="onClickOut"
        android:text="out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileLayoutDescriptionTextView" />

    <Button
        android:id="@+id/profileLayoutRightButton"
        android:layout_width="61dp"
        android:layout_height="150dp"
        android:background="@drawable/gradient"
        android:onClick="onClickRight"
        app:layout_constraintBottom_toBottomOf="@+id/profileLayoutPhotoImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toStartOf="@+id/profileLayoutPhotoImageView"
        app:layout_constraintTop_toTopOf="@+id/profileLayoutPhotoImageView"
        app:layout_constraintVertical_bias="0.484" />

    <Button
        android:id="@+id/profileLayoutLeftButton"
        android:layout_width="61dp"
        android:layout_height="150dp"
        android:background="@drawable/gradient"
        android:onClick="onClickLeft"
        app:layout_constraintBottom_toBottomOf="@+id/profileLayoutPhotoImageView"
        app:layout_constraintEnd_toEndOf="@+id/profileLayoutPhotoImageView"
        app:layout_constraintHorizontal_bias="0.056"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profileLayoutPhotoImageView"
        app:layout_constraintVertical_bias="0.484" />


</androidx.constraintlayout.widget.ConstraintLayout>